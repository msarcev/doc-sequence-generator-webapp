<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.1.xsd">

    <changeSet id="20161115000000-0" author="msarcevic">
        <createTable tableName="SEQUENCE">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints nullable="false" primaryKey="true" primaryKeyName="SEQUENCE_PK"/>
            </column>
            <column name="AUTHOR" type="VARCHAR(30)">
                <constraints nullable="false"/>
            </column>
            <column name="PURPOSE" type="VARCHAR(30)">
                <constraints nullable="true"/>
            </column>
            <column name="DATE" type="VARCHAR(30)">
                <constraints nullable="false"/>
            </column>
            <column name="FORMATTED_SEQ" type="VARCHAR(30)">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

</databaseChangeLog>